<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <properties resource="application.yml"/>
  <context id="context1" targetRuntime="MyBatis3Simple">
    <jdbcConnection driverClass="${driver-class-name}" 
    connectionURL="${url}" 
    userId="${username}" 
    password="${password}" />
    <javaModelGenerator targetPackage="com.gisuni.triggerlog.entity" 
    targetProject="${name}" />
    <sqlMapGenerator targetPackage="mapper" 
    targetProject="${name}\src\main\resources\" />
    <javaClientGenerator targetPackage="com.gisuni.triggerlog.mapper" 
    targetProject="${name}" 
    type="XMLMAPPER" />
    <table tableName="t_ums_user" />
  </context>
</generatorConfiguration>